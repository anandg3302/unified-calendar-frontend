# Updated .env for Expo with Railway backend

# Remove Emergent tunnel/subdomain settings
# EXPO_TUNNEL_SUBDOMAIN=calendarbridge-1
# EXPO_PACKAGER_HOSTNAME=https://calendarbridge-1.preview.emergentagent.com

# Backend URL pointing to Railway deployment
EXPO_PUBLIC_BACKEND_URL="http://127.0.0.1:8000"

# Keep fast resolver for Metro bundler
EXPO_USE_FAST_RESOLVER="1"

# Metro cache location (optional, keep as is)
METRO_CACHE_ROOT=/app/frontend/.metro-cache
